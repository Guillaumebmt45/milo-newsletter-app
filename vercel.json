{
  "functions": {
    "react-version/src/**": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "react-version/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/react-version/build/index.html"
    }
  ]
}